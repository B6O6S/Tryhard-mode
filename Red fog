-- Add red fog
local lighting = game:GetService("Lighting")
lighting.FogEnabled = true
lighting.FogColor = Color3.fromRGB(255, 0, 0) -- Red color
lighting.FogStart = 0
lighting.FogEnd = 500

-- Set red lighting
lighting.Ambient = Color3.fromRGB(255, 0, 0)  -- Red ambient lighting
lighting.OutdoorAmbient = Color3.fromRGB(255, 0, 0)  -- Red outdoor ambient lighting
